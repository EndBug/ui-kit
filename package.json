{
  "name": "ui-kit",
  "version": "0.0.0",
  "description": "UI component library based on our Design System",
  "homepage": "https://uikit.commercetools.com/",
  "bugs": "https://github.com/commercetools/ui-kit/issues",
  "private": true,
  "keywords": ["javascript", "frontend", "ui-kit", "design-system", "react"],
  "sideEffects": false,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "./scripts/postinstall.sh",
    "auth": "npm_config_registry=https://registry.npmjs.org npm whoami",
    "clean": "manypkg exec rm -rf dist",
    "generate-readmes": "pnpm generate-readme --all-workspace-packages",
    "generate-icons": "pnpm --filter icons run generate-icons && pnpm --filter checkbox-input run generate-icons && pnpm --filter rich-text-utils run generate-icons && prettier --write '**/generated/*.tsx'",
    "design-tokens:build": "pnpm --filter design-system run build:tokens",
    "design-tokens:build:watch": "pnpm --filter design-system run build:tokens:watch",
    "build": "./scripts/build.sh",
    "build:watch": "./scripts/build_watch.sh",
    "start": "pnpm --cwd docs start",
    "extract-intl": "formatjs extract --format=./packages/i18n/transifex-transformer.js --out-file=./packages/i18n/data/core.json '{packages,src/components}/**/messages.ts'",
    "compile-intl": "pnpm --filter @commercetools-uikit/i18n compile-data",
    "lint": "jest --projects jest.eslint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format:js": "prettier --write '**/*.{js,ts,tsx}'",
    "format:md": "prettier --write --parser markdown '**/*.md'",
    "format:yaml": "prettier --write --parser yaml '**/*.yaml'",
    "test": "jest --projects jest.{ts-test,test}.config.js",
    "test:watch": "pnpm test --watch",
    "test:bundle": "jest --config jest.bundle.config.js",
    "test:visual": "jest --config jest.visual.config.js --runInBand",
    "vrt:components": "percy exec -- pnpm test:visual",
    "bundlesize": "bundlesize",
    "test:bundle:watch": "jest --config jest.bundle.config.js --watch",
    "changeset": "changeset",
    "changeset:version-and-format": "changeset version && pnpm prettier --write --parser json '**/package.json' && SKIP_INSTALL_STORYBOOK=true pnpm install --lockfile-only",
    "visual-testing-app:start": "pnpm --filter visual-testing-app start",
    "visual-testing-app:build": "pnpm --filter visual-testing-app build",
    "visual-testing-app:preview": "pnpm --filter visual-testing-app preview",
    "labels:sync": "github-labels sync",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "preconstruct": {
    "packages": [
      "design-system",
      "packages/*",
      "packages/components/*",
      "packages/components/buttons/*",
      "packages/components/fields/*",
      "packages/components/inputs/*",
      "packages/components/spacings/*",
      "presets/*"
    ]
  },
  "dependencies": {
    "@babel/template": "7.22.5",
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@commercetools-frontend/babel-preset-mc-app": "21.25.2",
    "@commercetools-frontend/eslint-config-mc-app": "21.25.2",
    "@commercetools-frontend/ui-kit": "16.5.0",
    "@commercetools-local/generator-package-json": "*",
    "@commercetools-local/generator-readme": "*",
    "@commercetools-uikit/card": "16.5.0",
    "@commercetools-uikit/data-table-manager": "16.5.0",
    "@commercetools-uikit/design-system": "16.5.0",
    "@commercetools-uikit/grid": "16.5.0",
    "@commercetools-uikit/hooks": "16.5.0",
    "@commercetools-uikit/i18n": "16.5.0",
    "@commercetools-uikit/icons": "16.5.0",
    "@commercetools-uikit/pagination": "16.5.0",
    "@commercetools-uikit/rich-text-utils": "16.5.0",
    "@commercetools-uikit/spacings": "16.5.0",
    "@commercetools-uikit/text": "16.5.0",
    "@commercetools-uikit/utils": "16.5.0",
    "@commercetools/github-labels": "1.1.0",
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@emotion/styled": "^11.10.5",
    "@formatjs/cli": "6.1.3",
    "@formatjs/intl-relativetimeformat": "11.2.4",
    "@manypkg/cli": "0.21.0",
    "@manypkg/get-packages": "1.1.3",
    "@percy/cli": "1.26.2",
    "@percy/puppeteer": "2.0.2",
    "@preconstruct/cli": "2.6.4",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@svgr/babel-plugin-svg-dynamic-title": "^7.0.0",
    "@svgr/cli": "7.0.0",
    "@svgr/plugin-jsx": "7.0.0",
    "@svgr/plugin-svgo": "7.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "8.0.1",
    "@types/is-hotkey": "^0.1.7",
    "@types/jest": "29.5.1",
    "@types/node": "16.18.38",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-jest": "29.6.1",
    "babel-loader": "8.3.0",
    "babel-plugin-typescript-to-proptypes": "1.4.2",
    "browserslist": "4.21.9",
    "bundlesize": "0.18.1",
    "cac": "6.7.14",
    "colors": "1.4.0",
    "conventional-changelog-cli": "2.2.2",
    "cross-env": "7.0.3",
    "eslint": "8.44.0",
    "eslint-formatter-pretty": "4.1.0",
    "execa": "6.1.0",
    "find-up": "5.0.0",
    "formik": "^2.2.9",
    "glob": "8.1.0",
    "global": "4.4.0",
    "history": "4.10.1",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "intl-pluralrules": "1.3.1",
    "jest": "29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "jest-localstorage-mock": "2.4.26",
    "jest-puppeteer": "8.0.6",
    "jest-runner-eslint": "1.2.0",
    "jest-runner-stylelint": "2.3.7",
    "jest-silent-reporter": "0.5.0",
    "jest-validate": "29.6.1",
    "jest-watch-typeahead": "2.2.2",
    "lint-staged": "13.2.3",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.43",
    "omit-empty-es": "1.2.0",
    "postcss": "8.4.25",
    "postcss-syntax": "^0.36.2",
    "postcss-value-parser": "4.2.0",
    "pptr-testing-library": "0.7.0",
    "prettier": "2.8.8",
    "prop-types": "15.8.1",
    "puppeteer": "20.2.0",
    "qs": "6.11.2",
    "rcfile": "1.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^6.3.2",
    "react-router-dom": "5.3.4",
    "react-test-renderer": "17.0.2",
    "react-value": "0.2.0",
    "replace": "1.2.2",
    "rimraf": "3.0.2",
    "shelljs": "0.8.5",
    "stylelint": "15.10.2",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "5.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsc-files": "1.1.4",
    "typescript": "5.1.6"
  },
  "pnpm": {
    "overrides": {
      "@babel/preset-env": "^7.18.6",
      "@jest/types": "29.6.1",
      "@types/eslint": "^8.2.2",
      "@types/react": "17.0.62",
      "@types/react-dom": "17.0.20",
      "@types/react-router": "5.1.20",
      "@types/unist": "3.0.0",
      "@typescript-eslint/eslint-plugin": "5.62.0",
      "@typescript-eslint/parser": "5.62.0",
      "core-js-compat": "^3.23.4",
      "nwsapi": "2.2.5"
    },
    "patchedDependencies": {
      "babel-plugin-typescript-to-proptypes@1.4.2": "patches/babel-plugin-typescript-to-proptypes@1.4.2.patch",
      "typescript-react-function-component-props-handler@1.1.1": "patches/typescript-react-function-component-props-handler@1.1.1.patch"
    }
  },
  "engines": {
    "node": "16.x || >=18.0.0",
    "npm": ">=6",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.6.2"
}
